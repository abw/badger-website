{
  "name": "@abw/badger-website",
  "version": "0.2.1",
  "description": "Badger Powered Website",
  "type": "module",
  "main": "dist/badger-website.cjs",
  "module": "dist/badger-website.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/badger-website.js",
      "require": "./dist/badger-website.cjs"
    },
    "./config/*": "./dist/config/*",
    "./styles/*": "./dist/styles/*"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite --config vite.docs.js",
    "test": "vitest --ui --coverage",
    "build:css": "bin/build-css.sh",
    "build:docs": "vite build --config vite.docs.js",
    "build:lib": "vite build",
    "build": "$npm_execpath build:lib && $npm_execpath build:docs",
    "build:TODO": "$npm_execpath build:css && $npm_execpath build:lib && $npm_execpath build:docs",
    "preview": "vite preview --config vite.docs.js",
    "lint": "eslint 'web/**/*.{js,jsx}' 'src/**/*.{js,jsx}'"
  },
  "keywords": [
    "Badger",
    "React",
    "Website"
  ],
  "author": "Andy Wardley",
  "license": "ISC",
  "homepage": "https://abw.github.io/badger-website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abw/badger-website.git"
  },
  "bugs": {
    "url": "https://github.com/abw/badger-website/issues"
  },
  "dependencies": {
    "@abw/badger-css": "^1.1.17",
    "@abw/badger-react-ui": "^1.3.3",
    "@abw/badger-timestamp": "^1.1.0",
    "@abw/badger-utils": "^1.1.1",
    "@abw/react-context": "^2.0.4",
    "@abw/react-night-and-day": "^0.0.6",
    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@shikijs/transformers": "^1.4.0",
    "@testing-library/jest-dom": "^6.4.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.5.3",
    "@vitest/ui": "^1.5.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-refresh": "^0.4.6",
    "history": "^5.3.0",
    "jsdom": "^22.1.0",
    "rehype-mdx-code-props": "^3.0.1",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.76.0",
    "shiki": "^1.4.0",
    "vite": "^5.2.11",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.5.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
